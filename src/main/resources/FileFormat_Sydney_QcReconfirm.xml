<?xml version="1.0" encoding="UTF-8"?>
<FileFormat fileName="QC Reconfirm.xls" fileType="EXCEL" locale="en-US">
	<DataSet description="QC Reconfirm">
		<DataSheet sheetIndex="0">
			<DataRecord listSourceName="qcList" beginRow="1" endRow="99999">
				<BeginRecordCondition column="H" comparator="EQ" checkValue="TSR Name" firstRowAsData="FALSE" />

				<DataCell row="1" column="G" dataType="TEXT" fieldName="customerFullName" />
				<DataCell row="1" column="H" dataType="TEXT" fieldName="tsrFullName" />

				<DataCell row="1" column="E" dataType="DATE" fieldName="saleDate" dataFormat="dd/MM/yyyy" />
				<DataCell row="1" column="J" dataType="DATE" fieldName="qcStatusDate" dataFormat="dd/MM/yyyy" />
				<DataCell row="1" column="K" dataType="TEXT" fieldName="qcId" />
				<DataCell row="1" column="N" dataType="TEXT" fieldName="tsrStatus" />
				<DataCell row="1" column="O" dataType="TEXT" fieldName="qcStatus" />

				<DataCell row="1" column="P" dataType="TEXT" fieldName="reconfirmReason" />
				<DataCell row="1" column="Q" dataType="TEXT" fieldName="reconfirmRemark" />
				<DataCell row="1" column="R" dataType="TEXT" fieldName="currentReason" />
				<DataCell row="1" column="S" dataType="TEXT" fieldName="currentRemark" />

				<EndRecordCondition column="H" comparator="BLANK" />
			</DataRecord>
		</DataSheet>
	</DataSet>
</FileFormat>