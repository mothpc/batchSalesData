<?xml version="1.0" encoding="UTF-8"?>
<FileFormat fileName="Sales_Report_By_Records_Pending.xls" fileType="EXCEL" locale="en-US">
	<DataSet description="Sales_Report_By_Records">
<!-- 
		<DataSheet sheetName="Sales_Report_By_Records_Pending">
 -->
		<DataSheet sheetIndex="0">

			<DataCell row="2" column="B" dataType="TEXT" fieldName="campaignName" />
			<DataCell row="3" column="B" dataType="TEXT" fieldName="listLot" />
			<DataCell row="4" column="B" dataType="TEXT" fieldName="period" />
			<DataCell row="5" column="B" dataType="DATE" fieldName="printDate" dataFormat="dd/MM/yyyy H:mm:ss a" />

			<DataRecord listSourceName="salesRecord" beginRow="2" endRow="99999">
				<BeginRecordCondition column="F" comparator="EQ" checkValue="X-Ref" />
				<DataCell row="1" column="A" dataType="TEXT" fieldName="campaignName" />
				<DataCell row="1" column="B" dataType="TEXT" fieldName="campaignCode" />
				<DataCell row="1" column="C" dataType="TEXT" fieldName="listLotName" />
				<DataCell row="1" column="D" dataType="DATE" fieldName="approveDate" dataFormat="yyyy-MM-dd" />
				<DataCell row="1" column="E" dataType="DATE" fieldName="approveDatePending" dataFormat="yyyy-MM-dd" />
				<DataCell row="1" column="F" dataType="TEXT" fieldName="xRefference" />
				<DataCell row="1" column="G" dataType="NUMBER" fieldName="itemNo" dataFormat="0" /><!--  -->
				<DataCell row="1" column="H" dataType="TEXT" fieldName="customerFullName" />
				<DataCell row="1" column="I" dataType="TEXT" fieldName="product" />
				<DataCell row="1" column="J" dataType="NUMBER" dataFormat="#,##0.00" fieldName="premium" />
				<DataCell row="1" column="K" dataType="NUMBER" dataFormat="#,##0.00" fieldName="annualPremium" />
				<DataCell row="1" column="L" dataType="TEXT" fieldName="protectAmount" />
				<DataCell row="1" column="M" dataType="TEXT" fieldName="paymentChannel" />
				<DataCell row="1" column="N" dataType="TEXT" fieldName="paymentFrequency" />
				<DataCell row="1" column="O" dataType="TEXT" fieldName="qaStatus" />
				<DataCell row="1" column="P" dataType="TEXT" fieldName="qaReason" />
				<DataCell row="1" column="Q" dataType="DATE" fieldName="saleDate" dataFormat="yyyy-MM-dd" />
				<DataCell row="1" column="R" dataType="TEXT" fieldName="tsrCode" autoTrim="TRUE" />
				<DataCell row="1" column="S" dataType="TEXT" fieldName="tsrName" autoTrim="TRUE" />
				<DataCell row="1" column="T" dataType="TEXT" fieldName="supCode" autoTrim="TRUE" />
				<DataCell row="1" column="U" dataType="TEXT" fieldName="supName" autoTrim="TRUE" />
				<DataCell row="1" column="V" dataType="TEXT" fieldName="qaReasonDetail" />
				<EndRecordCondition column="F" comparator="BLANK" />
			</DataRecord>

		</DataSheet>
	</DataSet>
</FileFormat>